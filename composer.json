{
	"name" : "sonata-project/sandbox",
	"description" : "Sonata Project's sandbox (with ecommerce bundles)",
	"type" : "symfony-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Thomas Rabaix",
			"email" : "thomas.rabaix@sonata-project.org"
		}
	],
	"minimum-stability" : "stable",
	"require" : {
		"php" : "^7.1",
		"twig/twig" : "~1.12",
		"twig/extensions" : "~1.0",
		"sensio/distribution-bundle" : "v4.0.38",
		"sensio/framework-extra-bundle" : "~2.3",
		"sensio/generator-bundle" : "~2.4",
		"jms/security-extra-bundle" : "~1.5",
		"jms/di-extra-bundle" : "~1.4",
		"jms/serializer-bundle" : "1.5.0",
		"doctrine/orm" : "v2.5.6",
		"doctrine/dbal" : "~2.4",
		"doctrine/doctrine-bundle" : "1.6.3",
		"doctrine/doctrine-migrations-bundle" : "^1.0",
		"doctrine/migrations" : "v1.5.0",
		"doctrine/doctrine-fixtures-bundle" : "~2.2",
		"doctrine/data-fixtures" : "~1.0@dev",
		"symfony-cmf/routing-bundle" : "2.0.0",
		"knplabs/gaufrette" : "0.2.1",
		"knplabs/knp-menu-bundle" : "2.1.3",
		"symfony/config" : "^2.3",
		"symfony/console" : "^2.3",
		"symfony/form" : "2.8.28",
		"symfony/http-foundation" : "^2.3",
		"symfony/process" : "^2.3",
		"symfony/routing" : "^2.3",
		"symfony/security-bundle" : "^2.3",
		"symfony/twig-bridge" : "^2.3",
		"symfony/validator" : "^2.3",
		"symfony/symfony" : "2.8.28",
		"symfony/assetic-bundle" : "~2.3",
		"symfony/swiftmailer-bundle" : "~2.3",
		"symfony/monolog-bundle" : "~2.3",
		"sonata-project/easy-extends-bundle" : "2.2.0",
		"sonata-project/seo-bundle" : "2.3.0",
		"sonata-project/doctrine-extensions" : "1.0.2",
		"sonata-project/intl-bundle" : "2.3.1",
		"sonata-project/admin-bundle" : "3.24.0",
		"sonata-project/doctrine-orm-admin-bundle" : "3.1.5",
		"sonata-project/notification-bundle" : "3.1.0",
		"sonata-project/block-bundle" : "3.5.0",
		"sonata-project/media-bundle" : "3.3.1",
		"sonata-project/user-bundle" : "3.2.1",
		"sonata-project/cache-bundle" : "2.3.1",
		"sonata-project/cache" : "1.1.0",
		"sonata-project/page-bundle" : "3.5.2",
		"sonata-project/core-bundle" : "3.5.1@dev",
		"sonata-project/formatter-bundle" : "3.2.0",
		"sonata-project/news-bundle" : "3.1.2",
		"sonata-project/datagrid-bundle" : "2.2.1",
		"sonata-project/exporter" : "1.7.1",
		"sonata-project/timeline-bundle" : "~3.0.0",
		"sonata-project/classification-bundle" : "3.3.1",
		"sonata-project/comment-bundle" : "~3.0.0",
		"sonata-project/sonata-composer" : "dev-master",
		"sonata-project/ecommerce" : "2.1.1",
		"friendsofsymfony/comment-bundle" : "2.0.10",
		"simplethings/entity-audit-bundle" : "~0.5",
		"ahmedsamy/hype-mailchimp-bundle" : "v1.2",
		"willdurand/faker-bundle" : "~1.0",
		"mopa/bootstrap-bundle" : "v3.1.2",
		"twbs/bootstrap" : "v3.0.0",
		"stof/doctrine-extensions-bundle" : "~1.1",
		"stephpy/timeline-bundle" : "~2.0",
		"incenteev/composer-parameter-handler" : "~2.0",
		"egeloen/ckeditor-bundle" : "4.0.6"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.2",
		"behat/behat" : "~2.4",
		"behat/common-contexts" : "1.2.*@dev",
		"behat/mink-extension" : "~1.2",
		"behat/mink-goutte-driver" : "~1.0",
		"fabpot/goutte" : "~1.0",
		"behat/mink" : "~1.5",
		"behat/mink-browserkit-driver" : "~1.1",
		"behat/mink-selenium2-driver" : "~1.1",
		"matthiasnoback/symfony-config-test" : "~0.4",
		"matthiasnoback/symfony-dependency-injection-test" : "~0.7",
		"sllh/php-cs-fixer-styleci-bridge" : "^2.1",
		"symfony/phpunit-bridge" : "^2.7 || ^3.0"
	},
	"conflict" : {
		"jms/serializer" : "<0.13",
		"symfony/intl" : "<2.3.20"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		],
		"post-create-project-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"php bin/load_data.php"
		]
	},
	"autoload" : {
		"psr-0" : {
			"" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-0" : {
			"" : "src/"
		}
	},
	"config" : {
		"bin-dir" : "bin",
		"sort-packages" : true
	},
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-web-dir" : "web",
		"symfony-assets-install" : "symlink",
		"branch-alias" : {
			"dev-master" : "2.3.x-dev"
		},
		"incenteev-parameters" : [{
				"file" : "app/config/parameters.yml",
				"env-map" : {
					"database.driver" : "DATABASE_DRIVER",
					"database.host" : "DATABASE_HOST",
					"database.name" : "DATABASE_NAME",
					"database.user" : "DATABASE_USER",
					"database.password" : "DATABASE_PASSWORD",
					"mailer_transport" : "MAILER_TRANSPORT",
					"mailer_host" : "MAILER_HOST",
					"mailer_user" : "MAILER_USER",
					"mailer_password" : "MAILER_PASSWORD",
					"sonata_media.cdn.host" : "SONATA_MEDIA_CDN_HOST"
				}
			}
		]
	}
}