<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
    
       <service id="ryl.charity.admin.sponsor" class="Ryl\CharityBundle\Admin\SponsorAdmin">
          <tag name="sonata.admin" manager_type="orm" group="Ryl" label="Sponsors"/>
          <argument />
          <argument>Ryl\CharityBundle\Entity\Sponsor</argument>
          <argument />
      </service>

       <service id="ryl.charity.admin.project" class="Ryl\CharityBundle\Admin\ProjectAdmin">
          <tag name="sonata.admin" manager_type="orm" group="Ryl" label="Projects"/>
          <argument />
          <argument>Ryl\CharityBundle\Entity\Project</argument>
          <argument />
      </service>

   </services>
</container>